<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.stampsysback.mapper.StampSendRecordMapper">

    <insert id="insertStampSendRecord" parameterType="com.example.stampsysback.entity.StampSendRecord" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        INSERT INTO stamp_logs (user_id, stamp_id, room_id, sent_at)
        VALUES (#{userId}, #{stampId}, #{roomId}, #{sentAt})
    </insert>

</mapper>